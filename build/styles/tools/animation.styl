@keyframes toggleY
    0%
        transform translateY(-12.5%)
    50%
        transform translateY(12.5%)
    100%
        transform translateY(-12.5%)

@keyframes cursorToggle
    0%
        transform translateY(0)
    25%
        transform translateY(12.5%)
    50%
        transform translateY(0)
    100%
        transform translateY(0)

@keyframes lockToggle
    0%
        transform translateY(0)
    25%
        transform translateY(-12.5%)
    50%
        transform translateY(0)
    100%
        transform translateY(0)